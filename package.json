{
  "name": "@47ng/actions-clever-cloud",
  "version": "0.1.0-alpha.3",
  "private": true,
  "description": "GitHub Action to deploy to Clever Cloud",
  "main": "lib/main.js",
  "scripts": {
    "build": "tsc",
    "format": "prettier --write **/*.ts",
    "format-check": "prettier --check **/*.ts",
    "lint": "eslint src/**/*.ts",
    "test": "jest",
    "pack": "run-s pack:main pack:clever",
    "pack:main": "ncc build lib/main.js",
    "pack:clever": "ncc build node_modules/.bin/clever -o dist/clever",
    "all": "run-s build format lint pack test",
    "precommit": "npm run build && git add dist/"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/actions/typescript-action.git"
  },
  "keywords": [
    "actions",
    "node",
    "deploy",
    "deployment",
    "clever-cloud"
  ],
  "author": {
    "name": "Fran√ßois Best",
    "email": "contact@francoisbest.com",
    "url": "https://francoisbest.com"
  },
  "license": "MIT",
  "dependencies": {
    "@actions/core": "^1.2.0",
    "@actions/exec": "^1.0.3",
    "clever-tools": "^1.6.2",
    "ws": "^7.2.1"
  },
  "devDependencies": {
    "@types/jest": "^24.0.23",
    "@types/node": "^12.7.12",
    "@typescript-eslint/parser": "^2.8.0",
    "@zeit/ncc": "^0.20.5",
    "eslint": "^5.16.0",
    "eslint-plugin-github": "^2.0.0",
    "eslint-plugin-jest": "^22.21.0",
    "jest": "^24.9.0",
    "jest-circus": "^24.9.0",
    "js-yaml": "^3.13.1",
    "npm-run-all": "^4.1.5",
    "prettier": "^1.19.1",
    "ts-jest": "^24.2.0",
    "typescript": "^3.6.4"
  }
}
